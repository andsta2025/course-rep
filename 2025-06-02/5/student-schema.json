{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Student",
  "type": "object",
  "properties": {
        "name": {
         "type": "string"
        },
        "age": {
         "type": "integer",
         "minimum": 0
        },
        "grade": {
         "type": "string"
        },
        "subjects": {
         "type": "array",
         "items": {
          "type": "string"
        },
        "minItems": 1
      }
    },
    "required": ["name", "age", "grade", "subjects"]
}